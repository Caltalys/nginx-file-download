<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.persist.mapper.SdCardOrderRMapper">

    <resultMap type="com.example.domain.SdCardOrder" id="resultMap">
        <result property="createdAt" column="created_at" />
        <result property="updatedAt" column="updated_at" />
        <result property="priceFen" column="price_fen" />
    </resultMap>

    <select id="selectAll" resultMap="resultMap">
        SELECT * FROM sd_order WHERE enabled=true ORDER BY created_at DESC
    </select>

</mapper>